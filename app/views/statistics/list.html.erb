<div class="row">
	<div class="col-md-6 col-md-offset-3" >
		<div class="book-info">
			<div id="testdiv" >
			    <% @users.each do |u| %>
			    	<a href="/users/<%= u.id %>" class="links-user">
						<h2 class="book-title"><%= u.last_name %>&nbsp;&nbsp;<%= u.first_name %></h2>
					</a>
				<% end %>
				<a onclick="genPDF()" href="#" class = "btn-custom2">Version PDF</a>
			</div>
		</div>
	</div>
</div>

<script>
	function genPDF() {
	
	var doc = new jsPDF();
	
    var specialElementHandlers = {
        '#hidediv' : function(element,render) {return false;}
    };
    
    doc.fromHTML($('#testdiv').get(0), 20,20,{
                 'width':500,
        		'elementHandlers': specialElementHandlers
    });
	
	doc.save('users.pdf');
	
}
</script>